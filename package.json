{
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "playwright": "^1.56.0"
  },
  "scripts": {
    "test": "playwright test",
    "report": "playwright show-report",
    "chromium": "playwright test --project=chromium --headed",
    "firefox": "playwright test --project=firefox --headed",
    "webkit": "playwright test --project=webkit --headed",
    "trace": "playwright show-trace test-results/**/trace.zip",
